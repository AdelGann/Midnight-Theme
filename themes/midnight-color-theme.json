{
	"name": "Midnight",
	"type": "dark",
	"colors": {
		"editor.background": "#070709",
		"editor.foreground": "#ffffff",
		"activityBar.background": "#000000",
		"sideBar.background": "#000000da",
		"activityBar.border": "#29292d",
		"sideBar.border": "#29292d",
		"titleBar.border": "#29292d",
		"titleBar.activeBackground": "#000000",
		"titleBar.inactiveForeground": "#bcbcbd",
		"titleBar.activeForeground": "#ffffff",
		"sideBarTitle.foreground": "#bcbcbd",
		"tab.activeBackground": "#070709",
		"tab.inactiveBackground": "#0A0A0C",
		"tab.activeForeground": "#ffffff",
		"tab.inactiveForeground": "#bcbcbd",
		"statusBar.background": "#000000",
		"statusBar.foreground": "#bcbcbd",
		"list.activeSelectionBackground": "#1A1A1C",
		"list.hoverBackground": "#141416",
		"input.background": "#121214",
		"input.border": "#29292d",
		"panel.background": "#070709",
		"panel.border": "#29292d",
		"editorGroupHeader.tabsBackground": "#0A0A0C",
		"editorGroupHeader.border": "#29292d",
		"notifications.background": "#0A0A0C",
		"notifications.border": "#29292d"
	},
	"tokenColors": [
		{
			"scope": ["entity.name.type.typescript", "entity.name.type.js"],
			"settings": {
				"foreground": "#c792ea"
			}
		},
		{
			"scope": ["variable", "variable.other.object"],
			"settings": {
				"foreground": "#89ddff"
			}
		},
		{
			"scope": ["keyword.control", "storage.type", "keyword.operator"],
			"settings": {
				"foreground": "#e783d8"
			}
		},
		{
			"scope": ["string", "string.quoted.double", "string.quoted.single"],
			"settings": {
				"foreground": "#7fd0c4"
			}
		},
		{
			"scope": ["comment", "punctuation.definition.comment"],
			"settings": {
				"foreground": "#464b55",
				"fontStyle": "italic"
			}
		},
		{
			"scope": ["constant.numeric", "constant.language", "constant.character"],
			"settings": {
				"foreground": "#f78c6c"
			}
		},
		{
			"scope": [
				"entity.name.tag.html",
				"entity.name.tag.js.jsx",
				"entity.name.tag.tsx",
				"punctuation.definition.tag",
				"entity.name.tag"
			],
			"settings": {
				"foreground": "#ff5370",
				"fontStyle": "italic"
			}
		},
		{
			"scope": [
				"entity.other.attribute-name.html",
				"entity.other.attribute-name.jsx",
				"entity.other.attribute-name.tsx",
				"entity.other.attribute-name"
			],
			"settings": {
				"foreground": "#c3a6ff"
			}
		},
		{
			"scope": [
				"punctuation.definition.tag.begin",
				"punctuation.definition.tag.end",
				"punctuation.definition.tag.jsx",
				"punctuation.definition.tag.tsx",
				"punctuation.definition.tag.fragment"
			],
			"settings": {
				"foreground": "#4a4d56"
			}
		},
		{
			"scope": [
				"support.class.component.jsx",
				"support.class.component.tsx",
				"entity.name.type.jsx",
				"entity.name.type.tsx"
			],
			"settings": {
				"foreground": "#82aaff",
				"fontStyle": "italic bold"
			}
		},
		{
			"scope": ["support.type.property-name", "support.type.property-name.json"],
			"settings": {
				"foreground": "#ff9cac"
			}
		},
		{
			"scope": ["punctuation.separator.key-value"],
			"settings": {
				"foreground": "#e783d8"
			}
		},
		{
			"scope": ["punctuation.separator.comma"],
			"settings": {
				"foreground": "#89ddff"
			}
		},
		{
			"scope": ["punctuation.definition.block", "punctuation.definition.array"],
			"settings": {
				"foreground": "#89ddff"
			}
		},
		{
			"scope": ["support.function"],
			"settings": {
				"foreground": "#82aaff",
				"fontStyle": ""
			}
		},
		{
			"scope": ["support.class", "entity.name.class", "entity.name.type.class"],
			"settings": {
				"foreground": "#ffcb6b",
				"fontStyle": "italic"
			}
		},
		{
			"scope": ["meta.object-literal.key"],
			"settings": {
				"foreground": "#ff9cac"
			}
		},
		{
			"scope": ["support.type.property-name", "support.type.property-name.json"],
			"settings": {
				"foreground": "#ff9cac" // Color rosa suave para las keys
			}
		},
		{
			"scope": ["string.quoted.double.json", "string.quoted.single.json"],
			"settings": {
				"foreground": "#7fd083" // Color verde azulado para los values
			}
		},
		{
			"scope": ["punctuation.separator.key-value.json"],
			"settings": {
				"foreground": "#e783d8" // Color para los dos puntos
			}
		},
		{
			"scope": ["punctuation.separator.comma.json"],
			"settings": {
				"foreground": "#89ddff" // Color para las comas
			}
		},
		{
			"scope": [
				"punctuation.definition.dictionary.begin.json",
				"punctuation.definition.dictionary.end.json",
				"punctuation.definition.array.begin.json",
				"punctuation.definition.array.end.json"
			],
			"settings": {
				"foreground": "#89ddff" // Color para llaves y corchetes
			}
		}
	]
}
